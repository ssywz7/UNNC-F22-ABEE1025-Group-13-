   "source": [
    "a = int(input()) # enter the number of materials\n",
    "n = 0\n",
    "R = 0\n",
    "while n < a:\n",
    "    d = int(input()) # respectively enter the thickness of each material\n",
    "    k = int(input()) # respectively enter the thermal conductivity of each material\n",
    "    R += d / k\n",
    "    n += 1\n",
    "print(1 / R) # get U-value"
   ]
